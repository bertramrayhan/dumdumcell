	<subDataset name="laporanPembelian" uuid="a02ac494-16c9-4447-be26-6bf681e84eee">
		<parameter name="tanggalAwal" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="tanggalAkhir" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT 
    DATE_FORMAT(tb.tanggal_transaksi_beli, '%d-%m-%Y') AS tanggal_transaksi_beli,
    TIME(tb.tanggal_transaksi_beli) AS waktu_transaksi_beli,
    s.nama_supplier AS nama_supplier,
    b.merek AS nama_barang,
    dtb.jumlah_barang AS jumlah_barang,
    CONCAT('Rp', REPLACE(FORMAT(dtb.subtotal, 0), ',', '.')) AS subtotal,
    a.nama AS nama_admin
FROM ddckasir.transaksi_beli tb
JOIN ddckasir.detail_transaksi_beli dtb ON dtb.id_transaksi_beli = tb.id_transaksi_beli
JOIN ddckasir.barang b ON dtb.id_barang = b.id_barang
JOIN ddckasir.supplier s ON tb.id_supplier = s.id_supplier
JOIN ddckasir.admin a ON tb.id_admin = a.id_admin
WHERE DATE(tanggal_transaksi_beli) BETWEEN $P{tanggalAwal} AND $P{tanggalAkhir}]]>
		</queryString>
		<field name="tanggal_transaksi_beli" class="java.lang.String"/>
		<field name="waktu_transaksi_beli" class="java.sql.Time"/>
		<field name="nama_supplier" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="nama_barang" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="jumlah_barang" class="java.lang.Integer">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="subtotal" class="java.lang.String"/>
		<field name="nama_admin" class="java.lang.String"/>
	</subDataset>
	<subDataset name="total" uuid="39ddc3d1-3ca2-43a8-a436-f063491bebdd">
		<parameter name="tanggalAwal" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="tanggalAkhir" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT 
  COUNT(DISTINCT tb.id_transaksi_beli) AS total_transaksi,
  SUM(dtb.jumlah_barang) AS total_barang,
  CONCAT('Rp', REPLACE(FORMAT(SUM(tb.total_transaksi_beli), 0), ',', '.')) AS total_pembelian

FROM 
  transaksi_beli tb
JOIN 
  detail_transaksi_beli dtb ON tb.id_transaksi_beli = dtb.id_transaksi_beli
WHERE DATE(tb.tanggal_transaksi_beli) BETWEEN $P{tanggalAwal} AND $P{tanggalAkhir}]]>
		</queryString>
		<field name="total_transaksi" class="java.lang.Long"/>
		<field name="total_barang" class="java.math.BigDecimal"/>
		<field name="total_pembelian" class="java.lang.String"/>
	</subDataset>
	<parameter name="tanggalAwal" class="java.lang.String">
