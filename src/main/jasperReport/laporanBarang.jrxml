		<queryString>
			<![CDATA[SELECT 
  ks.id_barang,
  b.nama_barang,
  DATE_FORMAT(ks.tanggal, '%d-%m-%Y') AS tanggal, 
  ks.jumlah_masuk, 
  ks.jumlah_keluar, 
  ks.sisa, 
  ks.keterangan 
FROM kartu_stok ks
JOIN barang b ON b.id_barang = ks.id_barang
WHERE ks.tanggal BETWEEN $P{tanggalAwal} AND $P{tanggalAkhir}]]>
